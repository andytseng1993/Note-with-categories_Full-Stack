{
    "version": 2,
    "builds": [
      {
        "src": "./server/script.ts",  
        "use": "@vercel/node"
      },
      {
        "src": "./client/dist",    
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/script.ts"
      }
    ]
  }